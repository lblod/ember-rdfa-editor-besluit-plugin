<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{this.hasTitle}}
    {{on 'click' this.insertTitle}}>
    {{t "insert.decision-title"}}
  </AuButton>
</AuList::Item>
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    @disabled={{this.disableArticleInsert}}
    {{on "click" this.insertArticle}}>
    {{t "insert.article"}}
  </AuButton>
</AuList::Item>
