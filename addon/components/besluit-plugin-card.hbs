{{#unless this.hasTitle}}
  <AuList::Item>
    <AuButton
      @icon="add"
      @iconAlignment="left"
      @skin="link"
      {{on 'click' this.insertTitle}}>
      {{t "insert.decision-title"}}
    </AuButton>
  </AuList::Item>
{{/unless}}
<AuList::Item>
  <AuButton
    @icon="add"
    @iconAlignment="left"
    @skin="link"
    {{on "click" this.insertArticle}}>
    {{t "insert.article"}}
  </AuButton>
</AuList::Item>
